<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme.Light" parent="Theme.AppCompat.Light">
        <item name="colorPrimaryDark">#075E54</item>
        <item name="colorAccent">#8BC34A</item>

        <item name="colorPrimary">#075E54</item>
        <item name="colorPrimaryVariant">#075E54</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">#075E54</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>

    </style>

    <style name="AppTheme.Dark" parent="Theme.AppCompat">
        <item name="colorPrimary">#37474F</item>
        <item name="colorPrimaryDark">#263238</item>
        <item name="colorAccent">#8BC34A</item>
        <item name="colorPrimaryVariant">#075E54</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">#075E54</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>

    </style>

</resources>